{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nodemon ./bin/www --ignore vue",
    "start": "NODE_ENV=production node ./bin/www",
    "reset": "knex migrate:rollback --all && knex migrate:latest && knex seed:run",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "lint:check": "eslint .",
    "lint": "eslint --fix .",
    "oldtest": "mocha --require test/hooks.js --recursive --timeout 2000 --exit",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test-json": "mocha --require test/hooks.js --recursive --timeout 2000 --exit --reporter json > test-results.json"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "axios": "^1.7.9",
    "cjstoesm": "^3.0.0",
    "compression": "^1.8.0",
    "connect-history-api-fallback": "^2.0.0",
    "cookie-parser": "~1.4.7",
    "cors": "^2.8.5",
    "debug": "~4.4.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-mysql-session": "^3.0.3",
    "express-session": "^1.18.1",
    "helmet": "^8.0.0",
    "http-errors": "~2.0.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "morgan": "~1.10.0",
    "mysql": "^2.18.1",
    "node-cas-authentication": "github:alt-cs-lab/cas-authentication",
    "nodemailer": "^6.10.0",
    "nodemon": "^3.1.9",
    "objection": "^3.1.5",
    "pg": "^8.13.3",
    "pug": "^3.0.3",
    "supertest": "^7.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.0.5",
    "chai": "^5.2.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "mocha": "^11.1.0",
    "prettier": "^3.5.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "vitest": "^3.0.5"
  }
}
